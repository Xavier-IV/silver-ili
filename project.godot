; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Software Dev"
config/version="0.0.1"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GlobalSignal="*res://autoloaders/global_signal.gd"
GlobalVar="*res://autoloaders/global_var.gd"
GlobalDateTime="*res://autoloaders/global_date_time.gd"
GlobalCashflow="*res://autoloaders/global_cashflow.gd"
GlobalStat="*res://autoloaders/global_stat.gd"
GlobalJob="*res://autoloaders/global_job.gd"
GlobalModal="*res://autoloaders/global_modal.gd"
GlobalSave="*res://autoloaders/global_save.gd"
GlobalEvent="*res://autoloaders/global_event.gd"
Profile="*res://autoloaders/profile.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/dynamic_fonts/use_oversampling=true
